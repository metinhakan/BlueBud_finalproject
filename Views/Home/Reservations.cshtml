@{
    ViewData["Title"] = "Home Page";

}

@inject Microsoft.Extensions.Configuration.IConfiguration Configuration


@{
    ViewData["Title"] = "Home Page";
}

<div class="row"> <div class="col-md-6">
        <h3> The reserved Blue Buds </h3>
        @using BlueBud.Data        
        @using Microsoft.AspNetCore.Identity
        @model List<ChargerLocations>

        @if (ViewBag.ListFull == true)
        {
            <ul>
                @foreach (var charger in Model)
                {
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title">@charger.ChargerName</h5>
                            <h6 class="card-subtitle mb-2 text-muted">@charger.ChargerType</h6>
                            <a asp-controller="ChargerLocations" asp-action="CancelReserve" asp-route-id="@charger.Id" > <button type="button" class="btn btn-danger">Cancel Reservation</button></a>
                      
                        </div>
                    </div>
                }
            </ul>
         
        }
        else
        {
            <h3>All Blue Buds are avaliable</h3>
        }
        
        
    </div>



        
        
    </div>
    



